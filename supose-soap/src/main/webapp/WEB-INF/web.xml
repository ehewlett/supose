<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 

	<resource-env-ref>
	  <description>
		  <!--	    Object factory for DatabaseConnectionBean instances. -->
	  </description>
	  <resource-env-ref-name>
		  <!--    bean/DatabaseConnectionBean -->
	  </resource-env-ref-name>
	  <resource-env-ref-type>
		  <!-- DatabaseConnectionBean incl. packagenames -->
	  </resource-env-ref-type>
	</resource-env-ref>

	<description>The SupoSE SOAP Search API</description>
    <display-name>The SupoSE SOAP API</display-name>

    <servlet>
        <servlet-name>AxisServlet</servlet-name>
        <servlet-class>org.apache.axis2.transport.http.AxisServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
  <servlet-mapping>
    <servlet-name>AxisServlet</servlet-name>
    <url-pattern>/services/*</url-pattern>
  </servlet-mapping>
</web-app>
